@{
    string currentAction = (string) Url.RequestContext.RouteData.Values["Action"];
}
<div class="ui-helper-clearfix">
<ul class="menutab" style="margin-top:15px">
        <li><a href="@Url.Content("~/Home/Index")" title="WebUI4.Resources.Account.Home">
