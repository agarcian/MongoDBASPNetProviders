            @WebUI4.Resources.Account.ChangePassword
            </a>
        </li>
          @*<li><a href="@Url.Content("~/Account/LogOff")" title="WebUI4.Resources.Account.LogOff">
            <img src="http://media.altovientosolutions.com/dryicons/Handy%20Icons/24x24/eject.png" />
            @WebUI4.Resources.Account.LogOff
            </a>
        </li>*@
      }
      else
      { 
         <li><a href="@Url.Content("~/Account/RetrievePassword")" title="WebUI4.Resources.Account.RetrievePasswordTooltip">
            <img src="http://media.altovientosolutions.com/dryicons/Handy%20Icons/24x24/padlock_open.png" />
            @WebUI4.Resources.Account.RetrievePassword
            </a>
        </li>
          <li><a href="@Url.Content("~/Account/Register")" title="WebUI4.Resources.Account.RegisterTooltip">
            <img src="http://media.altovientosolutions.com/dryicons/Handy%20Icons/24x24/user.png" />
